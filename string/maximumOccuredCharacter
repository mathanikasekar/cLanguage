#include<stdio.h>
#include<string.h>
int main()
{
char a[100]="qazqwsx";
int c[26],i,l;
l=strlen(a);
for(i=0;i<26;i++)
    {
         c[i]=0;
    }
for(i=0;i<l;i++)
{
if (a[i] >= 'a' && a[i] <= 'z')
    {
    int x = a[i] - 'a';
    c[x]++;

    }
}

int max=0;
for(i=0;i<26;i++)
    {
        if(max<c[i])
        {
             max=c[i];
        }
    }

    int t=max;
    for(i=0;i<26;i++)
    {
        if(c[i]==t)
            printf("%c : %d",i+97,t);
    }

}
