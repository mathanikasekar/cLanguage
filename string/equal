#include<stdio.h>
#include<string.h>
int main()
{
  char a[20]="hello",b[20]="hello";
  int i,f=0;
  int l1=strlen(a),l2=strlen(b);
  if(l1!=l2)
    printf("Not equal");
  else
  {
      for(i=0;i<l1;i++)
      {
          if(a[i]==b[i])
          {
              f=0;
          }
          else
          {
            f=1;
          break;
          }

      }
      if(f==0)
        printf("equal");
      else
         printf("Not equal");
  }
}
