#include <stdio.h>
#include <string.h>
#include<malloc.h>

int main()
{
    int i,j;
    char *s=(char *)malloc(sizeof(char));
    scanf("%[^\n]%*c",s);
    int len=strlen(s);

    for(i=0;i<len-1;i++)
    {
        for(j=i+1;*(s+j)!='\0';j++) // not j<len it gives some error
        {
            if(*(s+i)!='*')  //already repeated character no need to check
            {
            if(*(s+i)==*(s+j))
            {
                printf("%c",*(s+i));
                *(s+j)='*'; //to prevent multiple times checking same character
                len--;
                break;
            }
            }
            else
                continue;
        }
    }
    return 0;
}
