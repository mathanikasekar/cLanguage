#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <malloc.h>

int main()
{
    char *s=(char *)malloc(sizeof(char));
    scanf("%[^\n]%*c",s);
    int i,count=0;
    for(i=0;*(s+i)!='\0';i++)
    {
        if((*(s+i))==' ')
            count++;
        else
            continue;
    }

    printf("%d",count+1);
    return 0;
}
