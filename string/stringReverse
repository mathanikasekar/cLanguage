#include <stdio.h>
#include <string.h>
#include <stdlib.h>
int main ()
{
     char a[10];
     gets(a);
     int l=strlen(a);
     int i=0,j=l-1;
     for(int i=l-1;i>=0;i--)
     {
         printf("%c",a[i]); // %c is important dont use %s...
     }
    return 0;
}


#include <stdio.h>
#include <string.h>
#include <stdlib.h>
int main ()
{
     char a[10];
     gets(a);
     int l=strlen(a);
     int i=0,j=l-1;
     printf("enter size:");
     while(i<j)
     {
     char t;
     t=a[i];
     a[i]=a[j];
     a[j]=t;
     i++;
     j--;
     }
     printf("/n%s",a);
     return 0;
}
