#include<stdio.h>
#include<string.h>
int main()
{
    char s[10]="deva apple",t[10];
    int len=strlen(s),i,j;
    j=0;
    for(i=0;i<len;i++)
    {
        if(s[i]!='A' && s[i]!='E' && s[i]!='I' && s[i]!='O' && s[i]!='U' && s[i]!='a' && s[i]!='e' && s[i]!='i' && s[i]!='o' && s[i]!='u')
        {
             s[j]=s[i];
             j++;
        }
    }
    printf("%s",s);
}
