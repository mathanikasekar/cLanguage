#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int sqr(int num)
{
    int s;
    s=num*num;
    return s;
}
int reverse(int num)
{
    int r,s;
    s=0;
    while(num!=0)
    {
        r=num%10;
        s=s*10+r;
        num=num/10;
    }
    return s;
}

int main()
{
    int n,t,ans,ans1,x,ans2;
    scanf("%d",&n);
    t=n;
    ans=sqr(n);
    x=reverse(t);
    ans1=sqr(x);
    ans2=reverse(ans1);
    if(ans==ans2)
    printf("Adam Number");
    else
    printf("Not a adam number");
    return 0;
}
