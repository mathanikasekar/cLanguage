#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int sqr(int num)
{
    int square;
    square=num*num;
    return square;
}
int reverse(int num)
{
    int r,reverse_number;
    reverse_number=0;
    while(num!=0)
    {
        r=num%10;
        reverse_number=reverse_number*10+r;
        num=num/10;
    }
    return reverse_number;
}

int main()
{
    int n,t,sqr_1,rev_1,sqr_2,rev_2;
    printf("Enter Number");
    scanf("%d",&n);
    t=n;
    sqr_1=sqr(n);
    rev_1=reverse(n);
    sqr_2=sqr(rev_1);
    rev_2=reverse(sqr_2);
    if(sqr_1==rev_2)
    printf("Adam Number");
    else
    printf("Not a adam number");
    return 0;
}
