#include<stdio.h>
int main()
{
int i,j,t,n;
printf("enter no of elements");
scanf("%d",&n);
int a[n];
for(i=0;i<n;i++)
{
    printf("enter elements");
    scanf("%d",&a[i]);
}
for(i=0;i<n;++i)
{
    for(j=i+1;j<n;++j)
    {
        if(a[i]>a[j])
        {
            t=a[i];
            a[i]=a[j];
            a[j]=t;
        }
    }
}
printf("elements after sorting");
for(i=0;i<n;++i)
{
    printf("%d ",a[i]);
}
}
