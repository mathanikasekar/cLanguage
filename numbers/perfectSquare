#include<stdio.h>
int main()
{
    int n=6,count=0;
    scanf("%d",&n);   //no of elts
    int a[6],i;
    int i,a[6]={6,25,36,4,8,7};
    for(i=0;i<n;i++)
    {
        scanf("%d ",&a[i]);  //getting i/p
    }
    for(i=0;i<n;i++)
    {
        int j;
        for(j=1;j<a[i];j++)
        {
            if(j*j==a[i])
                count++;  //no of elts having perfect sqre
        }
    }
    printf("%d",count);
}
