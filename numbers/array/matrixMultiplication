#include<stdio.h>
#include<string.h>
int main()
{
  int i,j,k,
  row,col;
  printf("enter row and col value\n");
  scanf("%d\n%d",&row,&col);
  int a[row][col],b[row][col],c[row][col];
  printf("enter value for 1st matrix\n");
  for(i=0;i<row;i++)
  {
      for(j=0;j<col;j++)
      {
          printf("enter value");
          scanf("%d",&a[i][j]);
      }
  }
  printf("enter value for 2nd matrix\n");
  for(i=0;i<row;i++)
  {
      for(j=0;j<col;j++)
      {
          printf("enter value");
          scanf("%d",&b[i][j]);
      }
  }
  for(i=0;i<row;i++)
  {
      for(j=0;j<col;j++)
      {
          c[i][j]=0;
        for(k=0;k<col;k++)
        {
            c[i][j]+=a[i][k]*b[k][j];
        }
      }
  }
  for(i=0;i<row;i++,printf("\n"))
  {
      for(j=0;j<col;j++)
      {
         printf("%d ",c[i][j]);
      }
  }
}
