with temporary varaiable :

#include <stdio.h>
void swap(int a,int b);
int main()
{
    int a,b;
    printf("Enter two numbers:");
    scanf("%d %d",&a,&b);
    printf("Before swapping:");
    printf("%d %d",a,b);
    swap(a,b);
    return 0;
}
void swap(int a,int b)
{
    int t=0;
    t=a;
    a=b;
    b=t;
    printf("\nAfter swapping:");
    printf("%d %d",a,b);
}

without temporary variable :

#include <stdio.h>
void swap(int a,int b);
int main()
{
     int a,b;
    printf("Enter two numbers:");
    scanf("%d %d",&a,&b);
    printf("Before swapping:");
    printf("%d %d",a,b);
    swap(a,b);
    return 0;
}
void swap(int a,int b)
{
    a=a+b;
    b=a-b;
    a=a-b;
    printf("\nAfter swapping:");
    printf("%d %d",a,b);
}

using bitwise operator :

#include <stdio.h>

int main()
{
    int a,b;
    scanf("%d %d",&a,&b);
    printf("before swapping : %d %d",a,b);
    a^=b;
    b^=a;
    a^=b;
    printf("\nafter swapping : %d %d",a,b);
    return 0;
}
