#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
    long n,t,rev,r;
    scanf("%ld",&n);
    t=n;
    rev=0;
    while(n>0)
    {
        r=n%10;
        rev=rev*10+r;
        n=n/10;
    }
    if(t==rev)
        printf("Palindrome");
    else
        printf("Not a Palindrome");
    return 0;
}
