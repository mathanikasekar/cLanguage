#include <stdio.h>
int main()
{
    int n,i,k;
    printf("Enter no of elements in the array\n");
    scanf("%d",&n);
    int a[n];
    printf("Enter elements in the array\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter element to be searched\n");
    scanf("%d",&k);
    int flag=0;
    for(i=0;i<n;i++)
    {
        if(a[i]==k)
        {
           flag++;
           break;
        }
    }
    if(flag==1)
      printf("element found");
    else
     printf("element not found");
    return 0;
}
