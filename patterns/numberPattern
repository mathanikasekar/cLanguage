  #include<stdio.h>
    int main()
    {
        int n=7,r,c;
        for(r=1;r<=n;r++,printf("\n"))
        {
        for(c=1;c<=n;c++)
        {
            if(r==1 || r==n ||c==1 || c==n ||r==c || r+c==n+1)
                printf("1");
            else
                printf("0");
    }}}

