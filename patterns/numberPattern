#include<stdio.h>
int main()
{
int n=5,r,c;

for(r=1;r<=n;r++,printf("\n"))
{
for(c=1;c<=n;c++)
{
    if(r==1 || r==n || c==1 || c==n)
    printf("1 ");
else
    printf("0 ");
}}}

