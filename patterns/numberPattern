#include<stdio.h>
int main()
{
    int n=5,r,c,count;//count to give condition row or column equal to n and n may be odd or even
    if(n%2==0)
        count=n;//if n is even
    else
        count=n+1;//if n is odd
    for(r=1;r<=n;r++,printf("\n"))
    {
    for(c=1;c<=n;c++)
    {
        if(r==count/2 || c==count/2)
                printf("0 ");
            else
                printf("1 ");
    }}}

